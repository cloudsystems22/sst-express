     <div class="row">
       <div class='col-md-6 col-sm-12'>
         <form action="" id="frmDadosLicensa">
           <div class="row">
             <div class="col-md-6">
               <input type="hidden" id="hddUserId" name="usuario_id" value='<%=usuarioAcesso[0].Usuario.id %>'>
               <input type="hidden" id="hddId" name="id">
               <label for="">CNPJ</label>
               <input type="text" name="cnpj" id="txtCnpj" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">IE</label>
               <input type="text" name="ie" id="txtIe" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Razão Social</label>
               <input type="text" name="razao_social" id="txtRazaoSocial" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Nome Fantasia</label>
               <input type="text" name="nome_fantasia" id="txtNomeFantasia" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">CNAE</label>
               <input type="text" name="cnae" id="txtCNAE" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">CEP</label>
               <input type="text" name="cep" id="txtCEP" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Logradouro</label>
               <input type="text" name="logradouro" id="txtLograd" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Número</label>
               <input type="text" name="numero" id="txtNumero" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Bairro</label>
               <input type="text" name="bairro" id="txtBairro" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Municipio</label>
               <input type="text" name="municipio" id="txtMunicipio" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Estado</label>
               <input type="text" name="estado" id="txtEstado" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Site</label>
               <input type="text" name="site" id="txtSite" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Fone</label>
               <input type="text" name="fone" id="txtFone" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Email</label>
               <input type="text" name="email" id="txtEmail" class="form-control">
             </div>
           </div>
         </form>

       </div>
