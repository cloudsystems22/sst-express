<%- include('./shared/header') %>

<main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
    <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2">Licensa</h1>
        <div class="btn-toolbar mb-2 mb-md-0">
          <div class="btn-group mr-2">
            <button type="button" class="btn btn-sm btn-outline-secondary">Atualizar Dados</button>
            <!-- <button type="button" class="btn btn-sm btn-outline-secondary">Exportar</button> -->
          </div>
          <!-- <button type="button" class="btn btn-sm btn-outline-secondary dropdown-toggle">
            <span data-feather="calendar"></span>
            Esta semana
          </button> -->
        </div>
     </div>
     <div class="row">
       <div class='col-md-6'>
         <form action="">
           <div class="row">
             <div class="col-md-6">
               <input type="hidden" name="niveis_acesso_usuario_id" value='<%=usuarioAcesso[0].Usuario[0].id %>'>
               <label for="">CNPJ</label>
               <input type="text" name="cnpj" id="txtCnpj" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">IE</label>
               <input type="text" name="ie" id="txtIe" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Razão Social</label>
               <input type="text" name="razao_social" id="txtRazaoSocial" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Nome Fantasia</label>
               <input type="text" name="nome_fantasia" id="txtNomeFantasia" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">CNAE</label>
               <input type="text" name="cnae" id="txtCNAE" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">CEP</label>
               <input type="text" name="cep" id="txtCEP" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Logradouro</label>
               <input type="text" name="logradouro" id="txtLograd" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Número</label>
               <input type="text" name="numero" id="txtNumero" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Bairro</label>
               <input type="text" name="bairro" id="txtBairro" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Municipio</label>
               <input type="text" name="municipio" id="txtMunicipio" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Estado</label>
               <input type="text" name="estado" id="txtEstado" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Site</label>
               <input type="text" name="site" id="txtSite" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Fone</label>
               <input type="text" name="fone" id="txtFone" class="form-control">
             </div>
             <div class="col-md-6">
               <label for="">Email</label>
               <input type="text" name="email" id="txtEmail" class="form-control">
             </div>
           </div>
         </form>

       </div>
       <div class="col-md-6">
         <form action="" id="frmLogo">
           <div class="row">
             <div class="col-md-12">
               <div id='logo-marca' style="background-image: url('../images/imginexist.png');">
                   <!-- <img src="./images/inexistente.png" alt=""> -->
               </div>

             </div>
             <div class="col-md-12">
                <input type="file" name="file" id="inputFile" class="form-control">
             </div>

           </div>

         </form>
       </div>
     </div>

</main>

 <!-- Requisições ajax -->
 <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
 <script src="./javascripts/crudLicensa.js"></script>

<%- include('./shared/footer') %>
